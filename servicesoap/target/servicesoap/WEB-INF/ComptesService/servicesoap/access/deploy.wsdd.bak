<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ComptesService WSDL service -->

  <service name="Comptes" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://access.servicesoap"/>
      <parameter name="wsdlServiceElement" value="ComptesService"/>
      <parameter name="schemaQualified" value="http://access.servicesoap"/>
      <parameter name="wsdlServicePort" value="Comptes"/>
      <parameter name="className" value="servicesoap.access.ComptesSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Comptes"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="listeComptesClient" qname="operNS:listeComptesClient" xmlns:operNS="http://access.servicesoap" returnQName="retNS:listeComptesClientReturn" xmlns:retNS="http://access.servicesoap" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="isCorrect" qname="operNS:isCorrect" xmlns:operNS="http://access.servicesoap" returnQName="retNS:isCorrectReturn" xmlns:retNS="http://access.servicesoap" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:nom" xmlns:pns="http://access.servicesoap" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:prenom" xmlns:pns="http://access.servicesoap" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:motdepasse" xmlns:pns="http://access.servicesoap" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:isClient" xmlns:pns="http://access.servicesoap" type="tns:boolean" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="listeComptesVendeur" qname="operNS:listeComptesVendeur" xmlns:operNS="http://access.servicesoap" returnQName="retNS:listeComptesVendeurReturn" xmlns:retNS="http://access.servicesoap" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="creerCompte" qname="operNS:creerCompte" xmlns:operNS="http://access.servicesoap" returnQName="retNS:creerCompteReturn" xmlns:retNS="http://access.servicesoap" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:nom" xmlns:pns="http://access.servicesoap" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:prenom" xmlns:pns="http://access.servicesoap" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:motdepasse" xmlns:pns="http://access.servicesoap" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:isClient" xmlns:pns="http://access.servicesoap" type="tns:boolean" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="creerCompte listeComptesVendeur listeComptesClient isCorrect"/>

  </service>
</deployment>
